<!-- header -->
<%-include('.. /partial/header.ejs')%>

	<body>
		<div class="wrapper">
			<!-- sidebar -->
			<%-include('.. /partial/left-menu.ejs')%>
				<div class="main">
					<!-- nav-bar -->
					<%-include('.. /partial/nav-bar.ejs')%>
						<main class="content">
							<div class="container-fluid p-0">
								<div class="row">
									<div class="col-12 col-xl-6">
										<div class="card">
											<div class="card-body">
												<form action="/admin/product/form/create" method="POST">
													<div class="mb-3">
														<label class="form-label">Email address</label>
														<input type="text" class="form-control" placeholder="Enter..."
															name="email">
													</div>
													<div class="mb-3">
														<label class="form-label">Password</label>
														<input type="password" class="form-control"
															placeholder="Enter..." name="password">
													</div>
													<div class="mb-3">
														<label class="form-label">Textarea</label>
														<textarea class="form-control" placeholder="Enter..." rows="3"
															name="dipcreption" spellcheck="false"></textarea>
													</div>
													<button type="submit" class="btn btn-primary">Submit</button>
												</form>
											</div>
										</div>
									</div>
								</div>
							</div>
						</main>
						<!-- footer -->
						<%-include('.. /partial/footer.ejs')%>
				</div>
		</div>
		<!-- js -->
		<%-include('.. /partial/js.ejs')%>
	</body>

	</html>